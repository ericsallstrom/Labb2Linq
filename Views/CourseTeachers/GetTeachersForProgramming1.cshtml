@model IEnumerable<Labb2Linq.Models.CourseTeacher>

@{
    ViewData["Title"] = "Programmering 1";
}

<h1>Kurs - @ViewData["Title"]</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Teacher)
            </th>                   
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var courseTeacher in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => courseTeacher.Teacher.TeacherName)
                </td>                                       
            </tr>
        }
    </tbody>
</table>